  "devDependencies": {
    "dev-null": "^0.1.1",
    "eslint": "^3.14.0",
    "espower-loader": "^1.2.0",
    "intelli-espower-loader": "^1.0.1",
    "isstream": "^0.1.2",
    "istanbul": "^0.4.5",
    "jscs": "^3.0.7",
    "leasot": "^4.3.0",
    "mocha": "^3.2.0",
    "mocha-clean": "^1.0.0",
    "node-static": "^0.7.9",
    "power-assert": "^1.4.2",
    "pre-commit": "^1.2.2",
    "random-string": "^0.1.2",
    "strip-ansi": "^3.0.1",
    "yargs": "^6.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ktty1220/cheerio-httpcli.git"
  },
